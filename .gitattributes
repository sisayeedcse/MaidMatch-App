# Auto detect text files and perform LF normalization
* text=auto

# Dart files
*.dart text

# Flutter/Dart specific
*.yaml text
*.yml text
pubspec.lock text

# Documentation
*.md text
*.txt text

# Binary files
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.mov binary
*.mp4 binary
*.mp3 binary
*.flv binary
*.fla binary
*.swf binary
*.gz binary
*.zip binary
*.7z binary
*.ttf binary
*.eot binary
*.woff binary
*.woff2 binary
*.pyc binary
*.pdf binary

# Archives
*.jar binary
*.war binary
*.ear binary
*.zip binary
*.tar.gz binary
*.rar binary

# Gradle
*.gradle text
gradlew text eol=lf
gradlew.bat text eol=crlf
